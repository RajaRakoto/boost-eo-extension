{
	"name": "boost-eo-extension",
	"displayName": "boost-eo",
	"version": "1.0.0",
	"description": "A collection of handy code snippets and design tricks to boost productivity for JavaScript developers, accessible within VSCode âœ¨",
	"publisher": "raja",
	"author": "Raja Rakotonirina <raja.rakoto7@gmail.com>",
	"license": "MIT",
	"icon": "./assets/boost-eo.png",
	"readme": "./README.md",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RajaRakoto/boost-eo-extension.git"
	},
	"homepage": "https://github.com/RajaRakoto/boost-eo-extension#readme",
	"bugs": "https://github.com/RajaRakoto/boost-eo-extension/issues",
	"files": [
		"dist"
	],
	"keywords": [
		"extension",
		"vscode",
		"javascript",
		"productivity",
		"snippets",
		"helper",
		"patterns",
		"collection"
	],
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Snippets"
	],
	"scripts": {
		"script:es6": "bun es6.js",
		"clean": "rimraf build dist coverage",
		"build": "bun run clean && bun build.js && bun src/index.js && vsce package && mv boost-eo-*.vsix dist",
		"test": "bun test",
		"eslint": "eslint --config .eslintrc --report-unused-disable-directives --max-warnings 0",
		"prettier": "prettier --config .prettierrc --write .",
		"backup": "grunt backup",
		"pkg-check": "depcheck",
		"pkg-upgrade": "ncu --interactive --format group --packageManager bun",
		"versioning": "ungit",
		"npm-version:major": "npm version major",
		"npm-version:minor": "npm version minor",
		"npm-version:patch": "npm version patch",
		"nvm": "nvm use"
	},
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./dist/boost-eo-extension.code-snippets"
			}
		]
	},
	"devDependencies": {
		"depcheck": "^1.4.7",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"grunt": "^1.6.1",
		"grunt-contrib-compress": "^2.0.0",
		"load-grunt-tasks": "^5.1.0",
		"npm-check-updates": "^16.14.15",
		"rimraf": "^5.0.5"
	}
}
